{
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "editor.formatOnSave": false,
  "search.exclude": {
    "**/node_modules": true,
    "package-lock.json": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "eslint.format.enable": true,
  "eslint.debug": true,
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true
  }
}